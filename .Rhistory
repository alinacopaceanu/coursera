pollutantmean <- function(directory, pollutant, id = 1:332) {
sum <- 0
count <- 0
for (i in id ) {
data <- read.csv(paste(directory, id, sep="/"))
sum <- sum + sum(data, na.rm = FALSE)
count <- count + sum(!is.na(data))
}
mean = sum / count
}
pollutantmean <- function(directory, pollutant, id = 1:332) {
sum <- 0
count <- 0
for (i in id ) {
data <- read.csv(paste(directory, id, sep="/"))
sum <- sum + sum(data[pollutant], na.rm = FALSE)
count <- count + sum(!is.na(data[pollutant]))
}
mean = sum / count
}
